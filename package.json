{
  "name": "react-website",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://davidkoojohn.github.io/react-website",
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gh-pages": "git push && npm run build && git add . && git commit -m 'chore: build demo' --allow-empty && git push && git subtree push --prefix=build origin gh-pages",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
}
